/* pages/view/view.wxss */
.box{
  width: 100%;
  height: 100vh;
  background-color: rgb(49, 74, 128);
  overflow: hidden;
}
.po{
  display: flex;
  /* justify-content: space-between; */
  align-items: center;
  background-color: #fff;
  height: 90rpx;
  box-sizing: border-box;
  padding: 0 30rpx;
  margin-top: 30rpx;

}
.text{
  width: 150rpx;
}
.field-index--van-field{
  background-color: #fff;
  padding: 0 !important;
}
.van-button{
  width: 98%;
  margin: auto;
}
.ne{
  text-align: center;
  margin-top: 40rpx;
}
.van-field__control{
  width: 400rpx !important;
}
.van-field__body{
  width: 100%;
}
.dh{
  box-sizing: border-box;
  padding:20rpx 30rpx ;
  color: rgb(78, 78, 78);
  border-bottom: 1px solid rgb(197, 197, 197);
}
.fl{
  display: flex;
  justify-content: space-between;
  color: black !important;
}